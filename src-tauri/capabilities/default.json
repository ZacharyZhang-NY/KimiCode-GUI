{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capabilities for Kimi GUI",
  "local": true,
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:event:default",
    "core:event:allow-listen",
    "core:event:allow-emit",
    "core:app:default",
    "core:window:default",
    "dialog:allow-open",
    "dialog:allow-save"
  ]
}
